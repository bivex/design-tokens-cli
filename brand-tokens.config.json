{
  "globalPrefix": "",
  "keepReferences": false,
  "preserveOrder": true,
  "outputColorFormat": "hex",
  "prefixes": {
    "color": "",
    "component": "",
    "shadow": "",
    "border": "",
    "typography": "",
    "spacing": "space-",
    "brand-identity": "",
    "brand-colors": "",
    "semantic-colors": "",
    "borders": "",
    "z-index": "z-"
  },
  "transforms": [
    {
      "name": "brand-system",
      "from": "brand-tokens",
      "to": [
        {
          "as": "css",
          "to": "brand-output/css"
        },
        {
          "as": "scss",
          "to": "brand-output/scss"
        },
        {
          "as": "mjs",
          "to": "brand-output/js"
        },
        {
          "as": "json",
          "to": "brand-output/json"
        },
        {
          "as": "tailwind-config",
          "to": "brand-output/tailwind"
        },
        {
          "as": "tailwind-theme",
          "to": "brand-output/tailwind"
        }
      ]
    }
  ]
}
