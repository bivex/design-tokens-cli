        {
          "as": "mjs",
          "to": "ui-kit-output/js"
        },
        {
          "as": "json",
          "to": "ui-kit-output/json"
        },
        {
          "as": "tailwind-config",
          "to": "ui-kit-output/tailwind"
        },
        {
          "as": "tailwind-theme",
          "to": "ui-kit-output/tailwind"
        }
      ]
    }
