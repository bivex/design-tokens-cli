{
  "globalPrefix": "",
  "keepReferences": true,
  "preserveOrder": true,
  "outputColorFormat": "hex",
  "prefixes": {
    "color": "",
    "typography": "",
    "spacing": "",
    "shadow": "",
    "border": "",
    "component": "",
    "opacity": "opacity"
  },
  "transforms": [
    {
      "name": "ui-kit",
      "from": "ui-kit-tokens",
      "to": [
        {
          "as": "css",
          "to": "ui-kit-output/css"
        },
        {
          "as": "scss",
          "to": "ui-kit-output/scss"
